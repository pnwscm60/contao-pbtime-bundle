{
    "name": "pnwscm60/contao-pbtime-bundle",
    "type": "contao-bundle",
    "description": "worktime extension for Contao",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Markus Schenker",
            "homepage": "https://github.com/pnwscm60"
        }
    ],
    "require": {
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "contao/easy-coding-standard": "^4.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^9.3",
        "symfony/phpunit-bridge": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Pnwscm60\\ContaoPbtimeBundle\\": "src/"
        }
    },
    "classmap": [
                   "src/Resources/contao/"
		],
		"exclude-from-classmap": [
                   "src/Resources/contao/config/",
                   "src/Resources/contao/dca/",
                   "src/Resources/contao/languages/",
                   "src/Resources/contao/templates/"
		],
    "extra": {
         "contao-manager-plugin": "Pnwscm60\\ContaoPbtimeBundle\\ContaoManager\\Plugin"
    },
	"version": "1.0"
}
